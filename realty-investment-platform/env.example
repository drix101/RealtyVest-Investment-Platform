# RealtyVest API Configuration
# Copy this file to .env and fill in your actual API keys

# Backend API Configuration
VITE_API_URL=http://localhost:3001/api

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Real Estate APIs
VITE_RENTSPREE_API_KEY=your_rentspree_api_key_here
VITE_ZILLOW_API_KEY=your_zillow_api_key_here
VITE_WALKSCORE_API_KEY=your_walkscore_api_key_here

# Financial APIs
VITE_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
VITE_FRED_API_KEY=your_fred_api_key_here
VITE_YAHOO_FINANCE_API_KEY=your_yahoo_finance_api_key_here

# Location & Mapping APIs
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_MAPBOX_API_KEY=your_mapbox_api_key_here

# Payment APIs
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
VITE_STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here 
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id_here
VITE_PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# Communication APIs
VITE_SENDGRID_API_KEY=your_sendgrid_api_key_here
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Authentication APIs
VITE_AUTH0_DOMAIN=your_auth0_domain_here
VITE_AUTH0_CLIENT_ID=your_auth0_client_id_here
VITE_AUTH0_CLIENT_SECRET=your_auth0_client_secret_here

# Google OAuth (for social login)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Facebook OAuth (for social login)
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here
VITE_FACEBOOK_APP_SECRET=your_facebook_app_secret_here

# Redirect URIs
VITE_REDIRECT_URI=http://localhost:5173/auth/callback

# RealtyVest Frontend Configuration
# No API keys needed for mock data deployment

# Environment
NODE_ENV=production
